<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App.Views.ItemsPage"
             Title="{Binding Title}"
             xmlns:local="clr-namespace:App.ViewModels"  
             xmlns:model="clr-namespace:App.Models"  
             x:Name="BrowseItemsPage">

    <ContentPage.ToolbarItems>
         <ToolbarItem IconImageSource="icon_plus.png" Clicked="Add_Attestation" /> 
    </ContentPage.ToolbarItems>
    
    <ScrollView>
        <StackLayout x:Name="stackLayout" Padding="10"></StackLayout>
    </ScrollView>
</ContentPage>
